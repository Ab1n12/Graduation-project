<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zwz.chat.dao.ImGroupDao">

    <select id="findBySearch" resultType="cn.zwz.chat.entity.ImGroup">
        select `im_group`.* from `im_group`

        where 1 = 1  order by time asc

    </select>

</mapper>